<p id="notice"><%= notice %></p>

<h1>Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Created</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.order(created_at: :desc).each do |feedback| %>
      <tr>
        <td><%= feedback.user %></td>
        <td><%= feedback.created_at.strftime("%m/%d/%Y") %></td>
        <td><%= link_to 'Show', feedback %></td>
        <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback', new_feedback_path %>
